<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml">
    <MenuBar>
        <Menu text="Profile">
            <MenuItem fx:id="editProfile" text="Edit Profile" />
        </Menu>
        <Menu text="Posts">
            <MenuItem fx:id="addPost" text="Add Post" />
            <MenuItem fx:id="retrievePost" text="Retrieve Posts" />
            <MenuItem fx:id="deletePost" text="Delete Post" />
        </Menu>
    </MenuBar>
</VBox>
